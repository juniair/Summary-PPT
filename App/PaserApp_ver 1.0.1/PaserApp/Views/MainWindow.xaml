<Window x:Class="PaserApp.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <ContentControl prism:RegionManager.RegionName="ButtonRegion" />
        <ContentControl prism:RegionManager.RegionName="JsonDataRegion" Grid.Column="1" />
        <ContentControl prism:RegionManager.RegionName="XmlDataRegion" Grid.Column="2" />
    </Grid>
</Window>
